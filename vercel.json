{
  "functions": {
    "Desktop/src/api/index.py": {
      "runtime": "@vercel/python"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/Desktop/src/api/index.py"
    },
    {
      "source": "/styles.css",
      "destination": "/public/styles.css"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}